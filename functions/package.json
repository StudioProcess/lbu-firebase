  "dependencies": {
    "es6-promise-pool": "^2.5.0",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-plugin-promise": "^4.0.1"
  },
  "private": true
}
